			<item label="Lock Computer">
				<action name="Execute">
					<execute>mate-screensaver-command -l</execute>
				</action>
			</item>
			<item label="Logout">
				<action name="Exit"/>
			</item>
			<item label="Restart">
				<action name="Restart"/>
			</item>
			<item label="POWER OFF">
				<action name="Execute">
					<execute>poweroff</execute>
				</action>
			</item>
		</menu>
	</menu>
</openbox_menu>
